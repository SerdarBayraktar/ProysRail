<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="13.7dp"
        android:layout_marginStart="21.7dp"
        android:fontFamily="@font/opensans_regular"
        android:textColor="@color/text_color"
        android:textSize="19.4sp"
        android:text="İmalat İsmi" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="22dp"
        android:layout_height="26.7dp"
        android:layout_marginTop="8.3dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="14.3dp"
        />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_marginTop="14dp"
        android:layout_below="@id/textView18"
        android:layout_width="match_parent"
        android:layout_height="0.3dp"
        app:srcCompat="@color/text_color" />
</RelativeLayout>