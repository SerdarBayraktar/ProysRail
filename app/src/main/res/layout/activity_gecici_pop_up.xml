<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Gecici_pop_up">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="@color/background" />

    <TextView
        android:id="@+id/contur0"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_alignBottom="@id/imageView10"
        android:background="#a3a3a3" />

    <TextView
        android:textAlignment="center"
        android:fontFamily="@font/opensans_semibold"
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25.3dp"
        android:layout_marginTop="5.3dp"
        android:text="İlerleme"
        android:textColor="@color/text_color"
        android:textSize="27.8sp" />



    <ImageView
        android:id="@+id/tick"
        android:layout_marginTop="19.7dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="14dp"
        android:layout_width="36dp"
        android:layout_height="26.3dp"
        app:srcCompat="@drawable/onay" />

    <ListView
        android:layout_below="@id/contur0"
        android:id="@+id/list_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/imalat_sec"
        android:paddingLeft="30dp"
        android:textAlignment="textStart"
        android:textSize="19.3sp"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:layout_width="match_parent"
        android:clickable="true"
        android:layout_height="50dp"
        android:layout_below="@id/list_view"
        android:text="İmalat Seç"
        android:background="#009640"
        />


</RelativeLayout>